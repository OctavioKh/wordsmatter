{
"manifest_version":2,
"name": "Words Matter",
"description": "Words Create Worlds",
"content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["popup.js"]
    }
  ],
"version":"1.0.0",
"icons":{"128":"icon_128.png"},
"browser_action":{
    "default_icon": "icon.png",
    "default_popup": "popup.html"
},
"permissions": ["activeTab"]


}